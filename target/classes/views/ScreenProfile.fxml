<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<StackPane cacheShape="false" centerShape="false" scaleShape="false" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ScreenProfile">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false">
         <children>
            <HBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0">
               <children>
                  <JFXTextField fx:id="fieldName" alignment="CENTER" cacheShape="false" centerShape="false" focusTraversable="false" labelFloat="true" promptText="Name" scaleShape="false">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font></JFXTextField>
                  <JFXButton cacheShape="false" centerShape="false" focusTraversable="false" onMouseClicked="#onUpdate" scaleShape="false" text="Update">
                     <font>
                        <Font name="Times New Roman" size="14.0" />
                     </font></JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
