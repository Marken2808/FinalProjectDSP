<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane cacheShape="false" centerShape="false" scaleShape="false" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ComponentCalendar">
   <children>
      <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" style="-fx-background-color: #e6e6e6;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" style="-fx-background-color: #cccccc;">
               <children>
                  <JFXButton fx:id="btPrev" alignment="CENTER" buttonType="RAISED" cacheShape="false" centerShape="false" contentDisplay="GRAPHIC_ONLY" focusTraversable="false" graphicTextGap="0.0" onAction="#previous" scaleShape="false" style="-fx-background-radius: 100;" text="Previous" HBox.hgrow="ALWAYS">
                     <graphic>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icon/chevron-left.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
                  <Label fx:id="labelTitle" alignment="CENTER" cacheShape="false" centerShape="false" focusTraversable="false" prefWidth="150.0" scaleShape="false" text="Text">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Label>
                  <JFXButton fx:id="btNext" alignment="CENTER" buttonType="RAISED" cacheShape="false" centerShape="false" contentDisplay="GRAPHIC_ONLY" focusTraversable="false" graphicTextGap="0.0" onAction="#next" scaleShape="false" style="-fx-background-radius: 50;" text="Next" HBox.hgrow="ALWAYS">
                     <graphic>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icon/chevron-right.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <GridPane fx:id="gpBody" alignment="CENTER" cacheShape="false" centerShape="false" hgap="5.0" onMouseClicked="#testGrid" scaleShape="false" style="-fx-background-color: white;" vgap="2.0" VBox.vgrow="ALWAYS">
               <columnConstraints>
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
                  <ColumnConstraints halignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="32.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" valignment="CENTER" />
               </rowConstraints>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </GridPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
