<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TabUser">
   <children>
      <TableView fx:id="tableUSER" editable="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="colUID" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="ID" />
          <TableColumn fx:id="colUUSERNAME" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Username" />
            <TableColumn fx:id="colUPASSWORD" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Password" />
            <TableColumn fx:id="colUSTATUS" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Status" />
            <TableColumn fx:id="colUROLE" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="ROLE" />
            <TableColumn fx:id="colUACTION" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="ACTION" />
        </columns>
      </TableView>
      <JFXButton contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" onMouseClicked="#onAddUser" prefHeight="40.0" prefWidth="40.0" style="-fx-background-radius: 25;" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icon/plus-circle.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
   </children>
</AnchorPane>
