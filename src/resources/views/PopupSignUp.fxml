<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PopupSignUp">
   <children>
      <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0">
         <children>
            <Label alignment="CENTER" cacheShape="false" centerShape="false" focusTraversable="false" scaleShape="false" text="Register">
               <font>
                  <Font name="Times New Roman" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0">
               <children>
                  <JFXTextField fx:id="fieldUsername" cacheShape="false" centerShape="false" focusTraversable="false" labelFloat="true" onKeyReleased="#isEmpty" prefColumnCount="10" promptText="Username" scaleShape="false">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font></JFXTextField>
                  <HBox alignment="CENTER_RIGHT" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0" visible="false">
                     <children>
                        <Label fx:id="textInfor" disable="true" text="abcccc" textAlignment="CENTER" textFill="RED">
                           <font>
                              <Font size="13.0" />
                           </font></Label>
                        <ImageView fx:id="picInfor" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icon/alert-circle.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0">
               <children>
                  <JFXPasswordField fx:id="fieldPassword" cacheShape="false" centerShape="false" focusTraversable="false" labelFloat="true" onKeyReleased="#isEmpty" prefColumnCount="10" promptText="Password" scaleShape="false">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font></JFXPasswordField>
                  <HBox alignment="CENTER_RIGHT" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0" visible="false">
                     <children>
                        <Label fx:id="textInfor" disable="true" text="abcccc" textAlignment="CENTER" textFill="RED">
                           <font>
                              <Font size="13.0" />
                           </font></Label>
                        <ImageView fx:id="picInfor" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icon/alert-circle.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0">
               <children>
                  <JFXPasswordField fx:id="fieldConfirm" cacheShape="false" centerShape="false" focusTraversable="false" labelFloat="true" onKeyReleased="#isEmpty" prefColumnCount="10" promptText="Confirm Password" scaleShape="false">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font></JFXPasswordField>
                  <HBox alignment="CENTER_RIGHT" cacheShape="false" centerShape="false" scaleShape="false" spacing="5.0" visible="false">
                     <children>
                        <Label fx:id="textInfor" disable="true" text="abcccc" textAlignment="CENTER" textFill="RED">
                           <font>
                              <Font size="13.0" />
                           </font></Label>
                        <ImageView fx:id="picInfor" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icon/alert-circle.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <AnchorPane cacheShape="false" centerShape="false" scaleShape="false">
               <children>
                  <JFXDatePicker fx:id="fieldDob" cacheShape="false" centerShape="false" defaultColor="BLACK" editable="false" focusTraversable="false" onAction="#checkingAge" scaleShape="false" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </JFXDatePicker>
               </children>
            </AnchorPane>
            <JFXSlider fx:id="sliderAge" cacheShape="false" centerShape="false" disable="true" focusTraversable="false" indicatorPosition="RIGHT" majorTickUnit="10.0" max="65.0" min="5.0" minorTickCount="1" pickOnBounds="false" scaleShape="false" showTickLabels="true" value="1.0">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <VBox.margin>
                  <Insets left="-5.0" top="-10.0" />
               </VBox.margin>
            </JFXSlider>
            <HBox alignment="CENTER" cacheShape="false" centerShape="false" scaleShape="false" spacing="10.0">
               <children>
                  <JFXButton fx:id="btnSignUp" buttonType="RAISED" cacheShape="false" centerShape="false" focusTraversable="false" onAction="#makeRegister" scaleShape="false" style="-fx-background-color: #00ffaa;" text="Sign Up">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font></JFXButton>
                  <JFXButton cacheShape="false" centerShape="false" focusTraversable="false" onAction="#goBack" text="Cancel">
                     <font>
                        <Font name="Times New Roman" size="14.0" />
                     </font></JFXButton>
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
