<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.CalendarController">
   <children>
      <StackPane layoutX="10.0" layoutY="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text fx:id="tHeader" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </StackPane>
      <GridPane fx:id="gpBody" alignment="CENTER" layoutX="38.0" layoutY="70.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#testGrid" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="40.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
            <RowConstraints prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
        </rowConstraints>
      </GridPane>
      <HBox fx:id="hbFooter" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <JFXButton fx:id="btPrev" onAction="#previous" text="Previous" />
            <JFXButton fx:id="btNext" onAction="#next" text="Next" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
   </padding>
</AnchorPane>
