<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.AttendanceController">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <LineChart fx:id="lineChart" prefWidth="400.0">
               <xAxis>
                  <CategoryAxis side="BOTTOM" fx:id="xline" />
               </xAxis>
               <yAxis>
                  <NumberAxis fx:id="yline" side="LEFT" />
               </yAxis>
            </LineChart>
            <BarChart fx:id="barChart" prefWidth="400.0">
               <xAxis>
                  <CategoryAxis side="BOTTOM" fx:id="xbar" />
               </xAxis>
               <yAxis>
                  <NumberAxis fx:id="ybar" side="LEFT" />
               </yAxis>
            </BarChart>
         </children>
      </HBox>
   </children>
</AnchorPane>
